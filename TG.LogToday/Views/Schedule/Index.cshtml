@model LogToday.Models.ScheduleIndexViewModel
@{
    ViewBag.Title = "Timer";
    Layout = "~/Views/Shared/_LayoutDevOops.cshtml";
}

@section styles{
    <link rel="stylesheet" media="print" href="@Url.Content("~/plugins/fullcalendar/fullcalendar.print.css")"/>
    <style>

    </style>
}

<!--Start Breadcrumb-->
<div class="row">
    <div id="breadcrumb" class="col-xs-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Home", "Index", "Dashboard")</li>
            <li>@Html.ActionLink("Agenda", "Index", "Schedule")</li>
        </ol>
    </div>
</div>
<!--End Breadcrumb-->

<div id="dashboard-header" class="row">
    <div class="col-xs-12 col-sm-12">
        <div class="box" style="opacity: 1; z-index: 1999; left: 0px; top: 0px;">
            <div class="box-header">
                <div class="box-icons">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content">
                <div class="row full-calendar">
                    <div class="col-sm-12">
                        <div id="calendar"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {
    @Scripts.Render(LogToday.BundleConfig.ViewsBundles.BUNDLES_SCHEDULE_INDEX)
}





